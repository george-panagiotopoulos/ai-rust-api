				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    pm.environment.set('JWT_TOKEN', response.access_token);",
									"    pm.globals.set('JWT_TOKEN', response.access_token);",
									"    pm.collectionVariables.set('JWT_TOKEN', response.access_token);",
									"    console.log('JWT Token saved to environment, globals, and collection variables');",
									"    console.log('Token expires in:', response.expires_in, 'seconds');",
									"    console.log('User:', response.user.username, '(Admin:', response.user.is_admin + ')');",
									"} else {",
									"    console.log('Login failed with status:', pm.response.code);",
									"    console.log('Response:', pm.response.text());",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
